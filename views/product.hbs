<head>
  <title>Product</title>
  <link rel="stylesheet" href="style/main_style.css">
  <link rel="stylesheet" href="style/product.css">
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Oswald:wght@500&display=swap" rel="stylesheet">
</head>
<div class="menu"><img src="./img/logo1.svg" alt="logo">
  <nav>
    <ul>
      <li><a href="\product">Product</a></li>
      <li><a href="\order">Order</a></li>
      <li><a href="\customers">Customers</a></li>
      <li><a href="\ ready_products">Ready Products</a></li>
      <li><a href="\archives">Archives</a></li><br><br>
      <li><a href="sing_out">Sing out</a></li>
    </ul>
  </nav>
</div>
<div class="main">
  <h1>Product</h1>
  <p>{{data}}</p>
  <div class="search">
    <form><input name="str_search" type="text"><select id="type_search_item" name="type_search_item">
        <option value="id">id</option>
        <option value="model">model</option>
        <option value="customers">customers</option>
      </select><button id="search_button"><img src="./img/transparency.svg" alt="search"></button></form>
  </div>
  <div class="table_product">
    <table>
      <tr>
        <th>ID</th>
        <th>MODEL</th>
        <th>CUSTOMERS</th>
        <th>DATA_START</th>
        <th>DATA FINISH</th>
      </tr>
      {{#each cust}}
      <tr>
        <td>{{this.id_product}}</td>
         <td>{{this.model}}</td>
        <td>{{this.id_order}}</td>
        <td>{{this.data_start}}</td>
        <td>{{this.data_finish}}</td>
      </tr>
      {{/each}}
     
    </table>
  </div>
</div>